{
  "extends": "@vue/tsconfig/tsconfig.json",
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.vue", "src/**/*.js"],
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "allowJs": true,
    "strict": false,
    "strictNullChecks": true,
    "types": ["vite/client"],
    "noImplicitAny": false,
    "paths": {}
  }
}
